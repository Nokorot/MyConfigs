                $BROWSER --app="https://www.messenger.com"
twitch          $BROWSER --app="https://www.twitch.com"
porn            i3-msg "workspace PP; gaps inner current set 0"; \
                google-chrome --new-window --incognito https://www.pornhub.com & \
                (sleep 1;  i3-msg "border pixel 0")
